{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test  --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock:server": "json-server --no-cors mocks/data.js",
    "bundle-report": "ng build -c=prod --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "commit": "cz",
    "format:fix": "pretty-quick --write --pattern \"src/**/*.{ts,html,json}\" --pattern \"e2e/**/*.{ts,html,json}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,html,json}\" \"e2e/**/*.{ts,html,json}\"",
    "generate:scam": "node npm-scripts/generate-scam.js",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix",
      "pre-push": "run-s test"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.1",
    "@angular/common": "~11.1.1",
    "@angular/compiler": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/forms": "~11.1.1",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/router": "~11.1.1",
    "@webcomponents/custom-elements": "^1.4.3",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.8.3",
    "date-fns": "^2.16.1",
    "rfdc": "^1.2.0",
    "rxjs": "~6.6.0",
    "ts-cacheable": "^1.0.3",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.3",
    "@ngx-formly/core": "^5.0.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~11.0.0",
    "@angular-devkit/build-angular": "~0.1102.12",
    "@angular/cli": "~11.1.2",
    "@angular/compiler-cli": "~11.1.1",
    "@ngneat/tailwind": "^6.0.0",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/typography": "0.2.0",
    "@trash-maker/local-schematics": "0.0.0",
    "@types/jasmine": "~3.7.4",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "faker": "^5.5.3",
    "husky": "^4.3.8",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "json-server": "^0.16.3",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "tailwindcss": "~1.9.6",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.3",
    "typescript": "4.1.5",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "chore": {
          "description": "ü§ñ Build process or auxiliary tool changes",
          "value": "chore"
        },
        "ci": {
          "description": "üé° CI related changes",
          "value": "ci"
        },
        "docs": {
          "description": "Ô∏è‚úèÔ∏è Documentation only changes",
          "value": "docs"
        },
        "feat": {
          "description": "üé∏ A new feature",
          "value": "feat"
        },
        "fix": {
          "description": "üêõ A bug fix",
          "value": "fix"
        },
        "perf": {
          "description": "Ô∏è‚ö° A code change that improves performance",
          "value": "perf"
        },
        "refactor": {
          "description": "üí° A code change that neither fixes a bug or adds a feature",
          "value": "refactor"
        },
        "release": {
          "description": "üèπ Create a release commit",
          "value": "release"
        },
        "style": {
          "description": "üíÑ Markup, white-space, formatting, missing semi-colons...",
          "value": "style"
        },
        "test": {
          "description": "üíç Adding missing tests",
          "value": "test"
        }
      }
    }
  }
}
