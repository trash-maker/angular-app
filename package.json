{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test  --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock:server": "json-server --no-cors mocks/data.js",
    "bundle-report": "ng build -c=prod --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "format:fix": "pretty-quick --write --pattern \"src/**/*.{ts,html,json}\" --pattern \"e2e/**/*.{ts,html,json}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,html,json}\" \"e2e/**/*.{ts,html,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix test"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.4",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "@webcomponents/custom-elements": "^1.4.3",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "classlist.js": "^1.1.20150312",
    "date-fns": "^2.16.1",
    "rxjs": "~6.6.0",
    "ts-cacheable": "^1.0.3",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular/cli": "~10.2.1",
    "@angular/compiler-cli": "~10.2.4",
    "@ngneat/tailwind": "^3.0.7",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "autoprefixer": "10.2.3",
    "codelyzer": "^6.0.0",
    "faker": "^5.1.0",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.16.3",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "8.2.4",
    "postcss-import": "14.0.0",
    "postcss-loader": "4.2.0",
    "postcss-scss": "3.0.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "tailwindcss": "^1.9.6",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
